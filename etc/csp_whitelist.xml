<?xml version="1.0" encoding="UTF-8"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="hipay-tpp" type="host">*.hipay-tpp.com</value>
                <value id="hipay" type="host">*.hipay.com</value>
                <value id="iesnare" type="host">mpsnare.iesnare.com</value>
                <value id="paypal" type="host">*.paypal.com</value>
                <value id="google" type="host">*.googleapis.com</value>
                <value id="oney-js" type="host">*.oney.io</value>
                <value id="oney-staging-js" type="host">*.staging.oney.io</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="hipay" type="host">*.hipay.com</value>
                <value id="google" type="host">*.googleapis.com</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="hipay-tpp" type="host">*.hipay-tpp.com</value>
                <value id="hipay" type="host">*.hipay.com</value>
                <value id="paypal" type="host">*.paypal.com</value>
                <value id="google" type="host">*.googleapis.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="hipay-tpp" type="host">*.hipay-tpp.com</value>
                <value id="hipay" type="host">*.hipay.com</value>
                <value id="iesnare" type="host">wss://mpsnare.iesnare.com</value>
                <value id="google" type="host">*.googleapis.com</value>
                <value id="oney-connect" type="host">*.oney.io</value>
                <value id="oney-staging-connect" type="host">*.staging.oney.io</value>
            </values>
        </policy>
        <policy id="media-src">
            <values>
                <value id="data" type="host">data:</value>
                <value id="iesnare" type="host">mpsnare.iesnare.com</value>
                <value id="google" type="host">*.storage.googleapis.com</value>
                <value id="google" type="host">*.googleapis.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="hipay" type="host">*.hipay.com</value>
                <value id="google" type="host">*.googleapis.com</value>
                <value id="oney-img" type="host">*.oney.io</value>
                <value id="oney-staging-img" type="host">*.staging.oney.io</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="oney-font" type="host">*.oney.io</value>
                <value id="oney-staging-font" type="host">*.staging.oney.io</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
