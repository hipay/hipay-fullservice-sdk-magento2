<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:App/etc/routes.xsd">
    <router id="admin">
        <route id="hipay" frontName="hipay">
            <module name="HiPay_fullserviceMagento" before="Magento_Backend"/>
        </route>
        <route id="hipay_rule" frontName="hipay_rule">
            <module name="HiPay_fullserviceMagento" before="Magento_Backend"/>
        </route>
    </router>
</config>
